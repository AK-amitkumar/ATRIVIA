<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="template_picking_innovative_layout">
            <!-- Multicompany -->
            <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"/>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"/>
            </t>
            <t t-call="general_template.picking_innovative_header"/>
            <t t-raw="0"/>
            <t t-call="report.external_layout_footer"/>
        </template>
        <template id="picking_innovative_header">
            <div class="header" style="background-color:#414243;padding: 10px;">
                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                    <tbody>
                        <tr>
                            <td>
                                <center>
                                    <table border="0" cellpadding="0" cellspacing="0" style="padding:0;font-family: Arial,Helvetica, sans-serif;" width="100%">
                                        <tbody>
                                            <tr>
                                                <td valign="top">
                                                    <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                                        <tr>
                                                            <td align="left" style="color:#4D4D4F; font-size:12px; padding: 0; margin:0 " valign="middle">
                                                                <img style="max-height: 80px;" t-att-src="'data:image/png;base64,%s' % o.company_id.invoice_logo or o.company_id.logo" t-if="o.company_id.logo"/>
                                                            </td>
                                                            <td align="center" style=" font-size:12px; padding:0px; margin:0;" valign="middle">
                                                                <img style="width:190px;height:35px;" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', o.name, 600, 100)"/>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                                <td t-attf-style="text-align:right;padding: 0 20px;background-color:#{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.theme_text_color or '#000000'};" valign="top" width="40%">
                                                    <h1 t-attf-style="text-transform:capitalize; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; font-size:30px; font-weight:bold;margin:0;text-transform:uppercase; margin-bottom:5px;line-height: 38px;letter-spacing: 2px;">
                                                        <span>Picking Operations</span>
                                                    </h1>
                                                    <b t-attf-style="font-size:15px;color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                        <span>Picking No:</span>
                                                    
                                                    <span t-field="o.name"/>
                                                    </b>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </center>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
        <template id="report_picking_innovative_main">
            <t t-call="general_template.template_picking_innovative_layout">
                <t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})"/>
                <div class="page">
                    <div class="oe_structure"/>
                    <table border="0" cellpadding="0" cellspacing="0" width="100%">
                        <tbody>
                            <tr>
                                <td>
                                    <center>
                                        <table border="5" cellpadding="0" cellspacing="0" style="padding:0px 0 20px; margin:0; font-family: Arial,Helvetica, sans-serif;" width="100%">
                                            <tr>
                                                <td align="left" style=" padding:10px 0 15px; line-height:20px;" valign="top">
                                                    <!-- Invocie To -->
                                                    <div style="padding: 0 0 20px;">
                                                        <t t-if="o.company_id and o.company_id.is_company_bold">
                                                            <h2 style="text-transform:capitalize; font-size :18px; padding:0px 0 5px; margin:0;">
                                                                <b>
                                                                    <span t-attf-style="color: #{o.company_id and o.company_id.company_color or '#ffffff'};" t-field="o.company_id.partner_id"/>
                                                                </b>
                                                            </h2>
                                                        </t>
                                                        <t t-if="not o.company_id.is_company_bold">
                                                            <h2 style="text-transform:capitalize; font-size :18px; padding:0px; margin:0;">
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.company_color or '#ffffff'};" t-field="o.company_id.partner_id"/>
                                                            </h2>
                                                        </t>
                                                        <p style="text-transform:capitalize;font-size :15px; margin:0; padding: 0;width: 60%;display: inline-block;">
                                                            <div t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">
                                                                <span t-esc="o._get_street(o.company_id.partner_id)"/>
                                                                <br/>
                                                                <span t-esc="o._get_address_details(o.company_id.partner_id)"/>
                                                            </div>
                                                            <t t-if="o.company_id.partner_id.mobile">
                                                                <strong t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">Mobile: </strong>
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};" t-field="o.company_id.partner_id.mobile"/>
                                                                <br/>
                                                            </t>
                                                            <t t-if="o.company_id.partner_id.email">
                                                                <strong t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">Email: </strong>
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};" t-field="o.company_id.partner_id.email"/>
                                                            </t>
                                                        </p>
                                                    </div>
                                                    <!-- End -->
                                                    <!-- Invoice To: -->
                                                    <p t-attf-style="text-transform:capitalize; color: #{o.company_id and o.company_id.theme_color or '#000000'}; font-size:15px;margin:0; margin-bottom:10px;">
                                                        <span>Picking Operations:</span>
                                                        <br/>
                                                        <span t-if="o.picking_type_id.code=='incoming' and o.partner_id">
                                                            <strong>Vendor Address:</strong>
                                                        </span>
                                                        <span t-if="o.picking_type_id.code=='internal' and o.partner_id">
                                                            <strong>Warehouse Address:</strong>
                                                        </span>
                                                        <span t-if="o.picking_type_id.code=='outgoing' and o.partner_id">
                                                            <strong>Customer Address:</strong>
                                                        </span>
                                                    </p>
                                                    <t t-if="o.company_id and o.company_id.is_customer_bold">
                                                        <h1 style="text-transform:capitalize; font-size :15px; padding:0px; margin:0;">
                                                            <b>
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.customer_color or '#ffffff'};" t-field="o.partner_id"/>
                                                            </b>
                                                        </h1>
                                                    </t>
                                                    <t t-if="not o.company_id.is_customer_bold">
                                                        <h1 style="text-transform:capitalize; font-size :15px; padding:0px; margin:0;">
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.customer_color or '#ffffff'};" t-field="o.partner_id"/>
                                                        </h1>
                                                    </t>
                                                    <p style="font-size :15px; margin:0; padding-top:0;">
                                                        <div t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">
                                                            <span t-esc="o._get_street(o.partner_id)"/>
                                                            <br/>
                                                            <span t-esc="o._get_address_details(o.partner_id)"/>
                                                        </div>
                                                        <t t-if="o.partner_id.mobile">
                                                            <b t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">Mobile:</b>
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};" t-field="o.partner_id.mobile"/>
                                                            <br/>
                                                        </t>
                                                        <t t-if="o.partner_id.email">
                                                            <b t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">Email:</b>
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};" t-field="o.partner_id.email"/>
                                                            <br/>
                                                        </t>
                                                        <t t-if="o.partner_id.vat">
                                                            <b t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">Vat:</b>
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};" t-field="o.partner_id.vat"/>
                                                            <br/>
                                                        </t>
                                                    </p>
                                                    <!-- End -->
                                                </td>
                                                <td align="right" style="font-size:14px; padding: 0px 0px 15px 15px; max-width:200px; width:200px; min-width:200px;">
                                                    <table border="0" cellpadding="0" cellspacing="0" t-attf-style="color:#fff; text-transform:capitalize; font-size:15px; border-collapse: collapse; width:100%;" wdith="100%">
                                                        <tr t-attf-style="color: #{o.company_id.text_color};">
                                                            <td style="padding:10px 0; border-bottom:1px solid #999; white-space: nowrap;">
                                                                <b>Order (Origin):</b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px; border-bottom:1px solid #999;">
                                                                <span t-field="o.origin"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id.text_color};">
                                                            <td style="padding:10px 0; border-bottom:1px solid #999;">
                                                                <b>State</b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px; border-bottom:1px solid #999;">
                                                                <span t-field="o.state"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id.text_color};">
                                                            <td style="padding:10px 0; border-bottom:1px solid #999;">
                                                                <b>
                                                                    <b>Commitment Date</b>
                                                                </b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px; border-bottom:1px solid #999;">
                                                                <span t-field="o.date"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id.text_color};">
                                                            <td style="padding:10px 0; border-bottom:1px solid #999;">
                                                                <b>Scheduled Date</b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px; border-bottom:1px solid #999;">
                                                                <span t-field="o.min_date"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id.text_color};" t-if="o._check_delivery_installed() and o.picking_type_id.code == 'outgoing'">
                                                            <td style="padding:10px 0; border-bottom:1px solid #999;">
                                                                <b>
                                                                    <b>Carrier</b>
                                                                </b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px; border-bottom:1px solid #999;">
                                                                <span t-field="o.carrier_id"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id.text_color};" t-if="o._check_delivery_installed() ">
                                                            <td style="padding:10px 0;">
                                                                <b>Weight</b>
                                                            </td>
                                                            <td style="padding:10px 0 10px 10px;">
                                                                <span t-field="o.weight"/>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                            </tr>
                                            <tr/>
                                            <tr>
                                                <td align="left" colspan="4" t-attf-style=" font-size:14px;line-height: 22px; padding:10px 0px 0px 0px; fnt-weight:bold; width:100%;" valign="top">
                                                    <table border="0" bordercolor="#{o.company_id and o.company_id.theme_color or '#000000'};" cellpadding="0" cellspacing="0" class="table table-bordered" t-attf-style="border:none; border-collapse: collapse; text-transform:capitalize; padding:0; margin:0; font-size: 15px;" t-if="o.pack_operation_ids" width="100%">
                                                        <thead>
                                                            <tr t-attf-style="page-break-inside: avoid !important;border-bottom: 3px solid #FFF; color: #019589;font-size: 17px;font-weight: bold; padding:10px 10px;">
                                                                <td align="left" t-attf-style="border:none;background-color: #414143;color: #{o.company_id and o.company_id.theme_text_color or '#000000'};  padding:15px" width="8%">Sr.</td>
                                                                <t t-if="o.company_id.add_product_image == True">
                                                                    <td align="left" t-attf-style="border:none;background-color: #414143;color: #{o.company_id and o.company_id.theme_text_color or '#000000'};  padding:15px" width="8%">Image</td>
                                                                </t>
                                                                <td align="left" t-attf-style="border:none;background-color: #414143;color: #{o.company_id and o.company_id.theme_text_color or '#000000'};padding:15px" width="52%">Product</td>
                                                                <td align="right" t-attf-style="border:none;background-color: #414143;color: #{o.company_id and o.company_id.theme_text_color or '#000000'};  padding:15px" width="14%">Quantity</td>
                                                                <td align="right" t-attf-style="border:none;background-color: #{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.theme_text_color or '#000000'};  padding:15px" t-if="o.picking_type_id.code != 'incoming'" width="15%">Source</td>
                                                                <td align="center" t-attf-style="border:none;background-color: #{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; padding:15px" width="12%">Barcode</td>
                                                                <td align="center" t-attf-style="border:none;background-color: #{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; padding:15px" t-if="o.picking_type_id.code != 'outgoing'" width="12%">Destination</td>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <div class="watermark text-center" style="position:absolute;opacity:0.25;font-size:1em;width:50%;z-index:1000;">
                                                                <img style="max-height: 100px;" t-att-src="'data:image/png;base64,%s' %  o.company_id.watermark_logo" t-if="o.company_id"/>
                                                            </div>
                                                            <t t-as="pack_operation" t-foreach="o.pack_operation_ids">
                                                                <t t-if="pack_operation_odd">
                                                                    <tr t-attf-style="'page-break-inside: avoid !important;background-color:#FFF'">
                                                                        <td align="left" class="text-center" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-esc="pack_operation_index+1"/>
                                                                        </td>
                                                                        <t t-if="o.company_id.add_product_image == True">
                                                                            <td class="text-left" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};" valign="middle"> 
                                                                                <span t-field="pack_operation.product_id.image_small" t-options="{&quot;widget&quot;:&quot;image&quot;,&quot;class&quot;:&quot;img-rounded&quot;}"/>
                                                                            </td>
                                                                        </t>
                                                                        <td class="text-left" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-field="pack_operation.product_id"/>
                                                                            <t t-if="not pack_operation.product_id and pack_operation.package_id">
                                                                                <span t-field="pack_operation.package_id"/>
                                                                            </t>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#dcdddf; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-field="pack_operation.product_qty"/>
                                                                            <span groups="product.group_uom" t-field="pack_operation.product_uom_id"/>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" t-if="o.picking_type_id.code != 'incoming'" valign="middle">
                                                                            <span t-field="pack_operation.location_id"/>
                                                                            <span t-if="pack_operation.package_id">:</span>
                                                                            <span t-field="pack_operation.package_id"/>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:5px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #FFF;" valign="middle">
                                                                            <span t-if="pack_operation.product_id and pack_operation.product_id.barcode">
                                                                                <img style="width:300px;height:50px" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.product_id.barcode, 600, 100)"/>
                                                                            </span>
                                                                            <span t-if="pack_operation.package_id and not pack_operation.product_id">
                                                                                <img style="width:300px;height:50px" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.package_id.name, 600, 100)"/>
                                                                            </span>
                                                                            <table t-attf-style="background-color:#{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.text_color or '#000000'};" class="table table-condensed" t-if="pack_operation.pack_lot_ids">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th>Lot</th>
                                                                                        <th>Qty</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tr t-as="packlot" t-foreach="pack_operation.pack_lot_ids">
                                                                                    <td style="border:0px">
                                                                                        <span t-field="packlot.lot_id"/>
                                                                                        <t t-if="not packlot.lot_id">
                                                                                            <span t-field="packlot.lot_name"/>
                                                                                        </t>
                                                                                    </td>
                                                                                    <td style="border:0px">
                                                                                        <span t-field="packlot.qty"/>
                                                                                        <t t-if="o.picking_type_id.use_existing_lots and o.state!='done'">/ 
                                                                                        
                                                                                            
                                                                                            
                                                                                            
                                                                                            <span t-field="packlot.qty_todo"/></t>
                                                                                    </td>
                                                                                </tr>
                                                                            </table>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:5px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id and o.company_id.text_color or '#000000'};" t-if="o.picking_type_id.code != 'outgoing'" valign="middle">
                                                                            <span t-field="pack_operation.location_dest_id"/>
                                                                            <span t-if="pack_operation.result_package_id">:</span>
                                                                            <span t-field="pack_operation.result_package_id"/>
                                                                        </td>
                                                                    </tr>
                                                                </t>
                                                                <t t-if="pack_operation_even">
                                                                    <tr t-attf-style="'page-break-inside: avoid !important;background-color:#FFF'">
                                                                        <td align="left" class="text-center" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-esc="pack_operation_index+1"/>
                                                                        </td>
                                                                        <t t-if="o.company_id.add_product_image == True">
                                                                            <td class="text-left" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};" valign="middle">
                                                                                <span t-field="pack_operation.product_id.image_small" t-options="{&quot;widget&quot;:&quot;image&quot;,&quot;class&quot;:&quot;img-rounded&quot;}"/>
                                                                            </td>
                                                                        </t>
                                                                        <td class="text-left" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-field="pack_operation.product_id"/>
                                                                            <t t-if="not pack_operation.product_id and pack_operation.package_id">
                                                                                <span t-field="pack_operation.package_id"/>
                                                                            </t>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#dcdddf; color: #{o.company_id.text_color};" valign="middle">
                                                                            <span t-field="pack_operation.product_qty"/>
                                                                            <span groups="product.group_uom" t-field="pack_operation.product_uom_id"/>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:8px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id.text_color};" t-if="o.picking_type_id.code != 'incoming'" valign="middle">
                                                                            <span t-field="pack_operation.location_id"/>
                                                                            <span t-if="pack_operation.package_id">:</span>
                                                                            <span t-field="pack_operation.package_id"/>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:5px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #FFF;" valign="middle">
                                                                            <span t-if="pack_operation.product_id and pack_operation.product_id.barcode">
                                                                                <img style="width:300px;height:50px" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.product_id.barcode, 600, 100)"/>
                                                                            </span>
                                                                            <span t-if="pack_operation.package_id and not pack_operation.product_id">
                                                                                <img style="width:300px;height:50px" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.package_id.name, 600, 100)"/>
                                                                            </span>
                                                                            <table t-attf-style="background-color:#{o.company_id and o.company_id.theme_color or '#000000'};color: #{o.company_id and o.company_id.text_color or '#000000'};" class="table table-condensed" t-if="pack_operation.pack_lot_ids">
                                                                                <thead>
                                                                                    <tr >
                                                                                        <th>Lot</th>
                                                                                        <th>Qty</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tr  t-as="packlot" t-foreach="pack_operation.pack_lot_ids">
                                                                                    <td style="border:0px">
                                                                                        <span t-field="packlot.lot_id"/>
                                                                                        <t t-if="not packlot.lot_id">
                                                                                            <span t-field="packlot.lot_name"/>
                                                                                        </t>
                                                                                    </td>
                                                                                    <td style="border:0px"> 
                                                                                        <span t-field="packlot.qty"/>
                                                                                        <t t-if="o.picking_type_id.use_existing_lots and o.state!='done'">/ 
                                                                                        
                                                                                            
                                                                                            
                                                                                            
                                                                                            <span t-field="packlot.qty_todo"/></t>
                                                                                    </td>
                                                                                </tr>
                                                                            </table>
                                                                        </td>
                                                                        <td class="text-right" t-attf-style="padding:5px 10px; border:none; border-bottom: 2px solid #{o.company_id and o.company_id.theme_color or '#000000'};background-color:#{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id and o.company_id.text_color or '#000000'};" t-if="o.picking_type_id.code != 'outgoing'" valign="middle">
                                                                            <span t-field="pack_operation.location_dest_id"/>
                                                                            <span t-if="pack_operation.result_package_id">:</span>
                                                                            <span t-field="pack_operation.result_package_id"/>
                                                                        </td>
                                                                    </tr>
                                                                </t>
                                                            </t>
                                                        </tbody>
                                                    </table>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <p t-attf-style="color: #{o.company_id and o.company_id.text_color or '#000000'};" t-if="not o.pack_operation_ids">You do not have any products reserved for this picking.  Please click the 'Reserve' button
                                    to check if products are available.
                                </p>
                                                </td>
                                            </tr>
                                        </table>
                                    </center>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="oe_structure"/>
                </div>
            </t>
        </template>
        <template id="report_picking_custom_innovative">
            <t t-call="report.html_container">
                <t t-as="o" t-foreach="docs">
                    <t t-call="general_template.report_picking_innovative_main" t-lang="o.partner_id.lang"/>
                </t>
            </t>
        </template>
    </data>
</odoo>